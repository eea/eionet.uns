<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.eurodyn.uns.model.MetadataElement" table="METADATA_ELEMENTS" lazy="false">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property name="name" type="java.lang.String" update="true" insert="true" column="NAME" length="255" not-null="true" unique="true" />
	</class>
</hibernate-mapping>
